<div class="row">
  <% @users.each do |user| %>
      <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
          <div class="caption">
            <h3><%= user.name %></h3>
            <h4><%= user.bio %></h4>
            <p><a href="<%=user_path(user)%>" class="btn btn-primary" role="button">Profile</a></p>
          </div>
        </div>
      </div>
  <% end %>
</div>
